PROGRAM IS
	n, j, ta[0:24], tb[0:24], tc[0:24]
BEGIN
	x := 24;
	n:=24;
	tc[0]:=n;
	tc[n]:=n- 24;
  j:=tc[0]+1;
  FOR i FROM tc[0] DOWNTO tc[n] DO
		ta[i]:=i+1;
		tb[i]:=j-i;
	ENDFOR
  j:=tc[n];
  WHILE j<tc[0] DO
		tc[j]:=ta[j]*tb[j];
    j:=j+1;
	ENDWHILE
  FOR k FROM 0 TO n DO
		WRITE tc[k];
	ENDFOR
END
