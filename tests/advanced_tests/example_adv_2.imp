PROCEDURE licz(T s, n) IS
  j, b, k, p
BEGIN
  p := 2;
  k := 10;
  j := 11;
  b := 13;
  n := 22;
  s[2]:=12;
  s[10] := 23;
  s[j] := 24;
  s[12] := j;
  s[b] := n;
  b := b+1;
  s[b] := n+j;
  b := b+1;
  s[b] := s[11];
  b := b+1;
  s[b] := s[k];
  b := b+1;
  s[b] := s[k] + s[p];
  b := b+1;
  s[n] := 14;
  s[b] := s[n] + s[n];
  WRITE s[10];

END
PROCEDURE wypisz(T s, n) IS
BEGIN
  FOR i FROM n DOWNTO 2 DO
    IF s[i]>0 THEN
      WRITE i;
    ENDIF
  ENDFOR
END
PROGRAM IS
  a, sito[2:100]
BEGIN
  a:=100;
  licz(sito,a);
  WRITE a;
    FOR i FROM 2 TO 25 DO
        WRITE sito[i];
    ENDFOR
END
