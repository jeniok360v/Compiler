PROCEDURE factorial(T s,n) IS
  sigma[2:4], p, wiu
BEGIN
  s[0]:=100000;
  p:=s[0];
  FOR i FROM 1 TO n DO
    s[i]:=p/i;
    p := s[i];
  ENDFOR
END

PROCEDURE bc(ni,k,m) IS
  si[0:100],p
BEGIN
  factorial(si,ni);
  p:=ni-k;
  m:=si[ni]/si[k];
  m:=m/si[p];
END

