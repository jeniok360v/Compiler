
PROGRAM IS
  a, sito[2:100], v, aa, g, c, vie, viie, d, n, p
BEGIN
  vie := 0- 1000000;
  viie := 2000;
  v := 9;
  aa:= 0- 9;
  g:= 5;
  c := 0- 5;
  a:=100;
  d:=0;
  n:=333;
  p:=24;
  sito[17] :=v*g;
  sito[18] :=v*c;
  sito[19] :=aa*g;
  sito[20] :=aa*c;
  sito[21] := d*c;
  sito[22] := n*aa;
  sito[p] := g*p;
  sito[25] := 11%2;
    FOR i FROM 17 TO 25 DO
        WRITE sito[i];
    ENDFOR

END
