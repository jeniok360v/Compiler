PROCEDURE factorial(T si,n) IS
  p
BEGIN
  si[0]:=1;
  p:=si[0];
  FOR i FROM 1 TO n DO
    si[i]:=p*i;
    p:=si[i];
  ENDFOR
END

PROCEDURE bc(n,k,m) IS
  s[0:100],p
BEGIN
  factorial(s,n);
  p:=n-k;
  m:=s[n]/s[k];
  m:=m/s[p];
END

PROGRAM IS
    n,k,w
BEGIN
    READ n;
    READ k;
    bc(n,k,w);
    WRITE w;
END
