PROCEDURE licz(T s, n) IS
  j, b
BEGIN
  FOR k FROM 14 TO 29 DO
    s[k] := 2;
    ENDFOR
  j := 3;
  b := j;
  j := n;
  n := 22;
  s[2]:=12;

END
PROCEDURE wypisz(T s, n) IS
BEGIN
  FOR i FROM n DOWNTO 2 DO
    IF s[i]>0 THEN
      WRITE i;
      WRITE s[i];
    ENDIF
  ENDFOR
END
PROGRAM IS
  a, sito[2:100]
BEGIN
  a:=100;
  licz(sito,a);
  WRITE a;
  wypisz(sito,a);
END
