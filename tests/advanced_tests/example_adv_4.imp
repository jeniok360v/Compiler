PROCEDURE licz(T s, n) IS
  j, b, k, p, v, g, a, c
BEGIN
  v := 9;
  a:= 0-9;
  g:= 5;
  c := 10-15;
  p := 2;
  k := 10;
  j := 11;
  b := 13;
  n := 22;
  s[b] := v%g;
  b := b+1;
  s[b] := v%c;
  b := b+1;
  s[b] := a%g;
  b := b+1;
  s[b] := a%c;
  WRITE v;
  WRITE g;
  WRITE a;
  WRITE c;

END
PROCEDURE wypisz(T s, n) IS
BEGIN
  FOR i FROM n DOWNTO 2 DO
    IF s[i]>0 THEN
      WRITE i;
    ENDIF
  ENDFOR
END
